    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "glob": "^7.1.7",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
